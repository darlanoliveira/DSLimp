    <body>
        <div id="main" class="container-fluid">
            <form>
                <div class="row">
                    <div class="form-group col-md-2">
                        <label for="Nome">Nome</label>
                        <input type="text" class="form-control" id="nomecli" placeholder="Nome do cliente">
                    </div>
                    
                    <div class="form-group col-md-2">
                        <label for="ContatoCliente">Contato no cliente</label>
                        <input type="text" class="form-control" id="contatocli" placeholder="Contato no cliente">
                    </div>

                    <div class="form-group col-md-2">
                        <label for="campo3">Bairro</label>
                        <input type="text" class="form-control" id="bairro" placeholder="Bairro">
                    </div>

                    <div class="form-group col-md-2">
                        <label for="campo3">Cidade</label>
                        <input type="text" class="form-control" id="cidade" placeholder="Cidade">
                    </div>

                    <div class="form-group col-md-2">
                        <label for="campo3">Telefone</label>
                        <input type="text" class="form-control" id="telefone" placeholder="Telefone">
                    </div>

                    <div class="form-group col-md-2">
                        <label for="campo3">CNPJ</label>
                        <input type="text" class="form-control" id="cnpj" placeholder="CNPJ">
                    </div>

                    
                    <div class="form-group col-md-6">
                        <label for="campo3">Endereço</label>
                        <input type="text" class="form-control" id="endereco" placeholder="Endereço">
                    </div>

                    <div class="form-group col-md-6">
                        <label for="campo3">Ponto de referência</label>
                        <input type="text" class="form-control" id="pontoreferencia" placeholder="Ponto de Referência">
                    </div>

                    <div class="form-group col-md-3">
                        <label for="pesquisarapida">Pesquisa rápida</label>
                        <input type="text" class="form-control" id="pesquisa" placeholder="Pesquisa Rápida">
                    </div>
                    <div>
                        <button type="button" class="btn btn-default" style="margin-top: 25px">
                            <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
                        </button>
                    </div>
                </div>
                
            </form>
            
            <div class="panel panel-default"></div>
            
            <div class="panel panel-default">
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>Nome do cliente</th>
